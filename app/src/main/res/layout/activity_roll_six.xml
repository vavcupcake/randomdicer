<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#78D7CD">

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="203dp"
        android:layout_height="380dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/dice_pink_6_5"
        tools:layout_editor_absoluteY="198dp" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="125dp"
        android:layout_height="96dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="24dp"
        android:rotation="180"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/dice_pink_6_6" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="76dp"
        android:layout_height="26dp"
        android:layout_marginTop="8dp"
        android:rotation="180"
        android:text="before"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView19"
        app:layout_constraintStart_toStartOf="@+id/imageView19"
        app:layout_constraintTop_toBottomOf="@+id/imageView19" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="125dp"
        android:layout_height="96dp"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/dice_pink_6_6" />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="144dp"
        android:layout_height="62dp"
        android:layout_marginLeft="24dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/random_dice_button_6" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="76dp"
        android:layout_height="26dp"
        android:layout_marginBottom="8dp"
        android:text="before"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView26"
        app:layout_constraintEnd_toEndOf="@+id/imageView26"
        app:layout_constraintStart_toStartOf="@+id/imageView26" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="144dp"
        android:layout_height="62dp"
        android:rotation="180"
        app:srcCompat="@drawable/random_dice_button_6"
        tools:layout_editor_absoluteX="243dp"
        tools:layout_editor_absoluteY="24dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="94dp"
        android:layout_height="84dp"
        android:rotation="180"
        android:text="5"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="188dp" />

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="73dp"
        android:layout_height="162dp"
        android:layout_marginStart="15dp"
        android:rotation="180"
        app:layout_constraintBottom_toBottomOf="@+id/imageView21"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView21"
        app:srcCompat="@drawable/roll_button_6" />

    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="73dp"
        android:layout_height="162dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView21"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.521"
        app:srcCompat="@drawable/roll_button_6" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="94dp"
        android:layout_height="84dp"
        android:rotation="0"
        android:text="5"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="494dp" />

</androidx.constraintlayout.widget.ConstraintLayout>